<table id="myTable" class="table table-striped table-bordered">
   
    <% for( let i = 0; i < 1; i++ ) { %>
        <thead class="text-center">
            <tr >
                <% for( let j =0; j< data[i].length; j++ ) { %>
                    <th scope="col"><%= data[i][j] %> 
                        
                        <div class="sort d-flex justify-content-center align-items-center mt-1 me-1">                           
                            <h5>
                                <i col="<%= j %>" class="bi bi-caret-up-fill text-primary ascending pointer" title="ascending"></i> 
                            </h5>
                            
                            <h5>
                                <i col="<%= j %>" class="bi bi-caret-down-fill text-primary descending pointer me-1" title="descending"></i> 
                            </h5>
                            
                            <h6>
                                <i col="<%= j %>" class="chart bi bi-pie-chart-fill pointer" title="chart"></i> 
                            </h6>
                        </div>
                    </th>
                <% } %>
            </tr>
        </thead>
    <% } %>
    <tbody class="tbody text-center">

    <% if (data.length <= 100) { %>
        <% for( let i = 1; i < data.length; i++ ) { %>
        
            <tr>
                <% for( col of data[i] ) { %>
                    <td><%= col %> </td>
                <% } %>
            </tr>
            
        <% } %>    
    <% } else { %>
        <% for( let i = 1; i <= 100; i++ ) { %>
        
            <tr>
                <% for( col of data[i] ) { %>
                    <td><%= col %> </td>
                <% } %>
            </tr>
            
        <% } %>
    <% } %> 
    
    </tbody>
</table>

